{
    "langrev": "1",
    "warnings": ["no-unused"],
    "dependency-search-paths": [],
    "dependencies": [],
    "authors": ["LowByteFox"],
    "version": "0.0.1",
    "output": "build",
    "target": "elf-x86",
    "targets": {
        "libkernel": {
            "type": "static-lib",
            "sources": ["kernel/**"]
        },
        "libc": {
            "type": "object-files",
            "sources": ["libc/**"]
        }
    },
    "cpu": "generic",
    "opt": "O0",
    "use-stdlib": false,
    "no-entry": true,
    "link-libc": false,
    "safe": false,
    "strip-unused": false
}
